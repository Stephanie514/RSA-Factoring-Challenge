#!/usr/bin/python3

import sys
import time
import math

def is_prime(num):
    if num < 2:
        return False
    for i in range(2, int(math.sqrt(num)) + 1):
        if num % i == 0:
            return False
    return True

def factorize(num):
    factors = None
    for a in range(2, num):
        if num % a == 0 and is_prime(a) and is_prime(num // a):
            factors = (a, num // a)
            break
    return factors

def main():
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        return

    input_file = sys.argv[1]

    try:
        with open(input_file, 'r') as f:
            lines = f.readlines()

        for line in lines:
            num = int(line.strip())
            factors = factorize(num)
            if factors:
                print(f"{num}={factors[0]}*{factors[1]}")
            else:
                print(f"{num} is prime.")

    except FileNotFoundError:
        print(f"File not found: {input_file}")
        return

if __name__ == '__main__':
    main()
